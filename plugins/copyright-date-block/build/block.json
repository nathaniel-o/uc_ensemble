{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "untouched-cocktails/copyright-date-block",
  "version": "0.1.0",
  "title": "Copyright Date Block",
  "category": "widgets",
  "icon": "calendar",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "supports": {
    "html": false,
    "align": true,
    "color": true
  },
  "textdomain": "copyright-date-block",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "attributes": {
    "startingYear": {
      "type": "string",
      "default": "2000"
    },
    "showStartYear": {
      "type": "boolean",
      "default": true
    },
    "showCopyright": {
      "type": "boolean",
      "default": true
    }
  },
  "render": "file:./render.php"
}